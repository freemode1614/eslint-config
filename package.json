{
  "name": "@moccona/eslint-config",
  "version": "0.4.0",
  "description": "A simple eslint configuration",
  "main": "./index.js",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    "./package.json": "./package.json",
    "./flat/*": {
      "types": "./lib/flat/*.d.ts",
      "import": "./lib/flat/*.js"
    },
    ".": {
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "test": "cross-env NODE_ENV=production vitest --watch false",
    "prepublishOnly": "pnpm test",
    "prepare": "pnpm build",
    "build": "tsc",
    "lint": "eslint ./src/*"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint flat config"
  ],
  "author": "wp.l",
  "license": "MIT",
  "dependencies": {
    "@eslint/js": "^9.23.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/eslint-plugin": "^1.1.43",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-project-structure": "^3.13.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-714736e-20250131",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unicorn": "^58.0.0",
    "fs-extra": "^11.3.0",
    "globals": "^16.0.0",
    "semver": "^7.7.1",
    "typescript-eslint": "^8.28.0"
  },
  "devDependencies": {
    "@changesets/cli": "latest",
    "@moccona/logger": "^0.0.2",
    "@moccona/preset-config": "^0.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.28",
    "@types/semver": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "pkg-types": "^2.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "eslint": "^9 || ^8.23"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.9.0"
}
