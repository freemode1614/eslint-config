{
  "name": "@moccona/eslint-config",
  "version": "1.0.2",
  "description": "A simple eslint configuration",
  "main": "./index.js",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    "./package.json": "./package.json",
    "./legacy": {
      "types": "./dist/legacy.d.ts",
      "import": "./dist/legacy.js"
    },
    "./extra/*": {
      "types": "./dist/extra/*.d.ts",
      "import": "./dist/extra/*.js"
    },
    ".": {
      "types": "./dist/flat.d.ts",
      "import": "./dist/flat.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "rimraf ./dist && tsc",
    "test": "cross-env NODE_ENV=production vitest --watch false",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint flat config"
  ],
  "author": "wp.l",
  "license": "MIT",
  "dependencies": {
    "@eslint/js": "^9.24.0",
    "@moccona/logger": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitest/eslint-plugin": "^1.1.39",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-project-structure": "^3.13.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.0.0-beta-e993439-20250405",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "fs-extra": "^11.3.0",
    "globals": "^15.15.0",
    "semver": "^7.7.1",
    "typescript-eslint": "^8.29.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@moccona/preset-config": "^0.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.30",
    "@types/semver": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "pkg-types": "^1.3.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "eslint": "^9 || ^8.23"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.7.1"
}
